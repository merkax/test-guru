<tr>
  <td><%= link_to test.title, admin_test_path(test) %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('buttons.edit'), edit_admin_test_path(test), class: "btn btn-outline-info" %></td>
  <td>
      <%= link_to t('buttons.delete'),
                  admin_test_path(test),
                  method: :delete,
                  data:   { confirm: t('delete_confirm')}, class: "btn btn-outline-info" %></td>
</tr>
